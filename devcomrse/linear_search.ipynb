    "def linear_search(L, x):\n",
    "    i = 0\n",
    "    while i < len(L) and L[i] !=x:\n",
    "        i += 1\n",
    "    if i < len(L):\n",
    "        return i\n",
    "    else:\n",
    "        return -1"
